//Setup constants
const JSON = require('JSON');
const encodeUri = require('encodeUri');
const logToConsole = require('logToConsole');
const sendHttpRequest = require('sendHttpRequest');

//New/returned customer. 0 = returned, 1 = new
let new_customer ;
if (typeof data.oldCustomer != 'undefined' && (data.oldCustomer == true || data.oldCustomer >= 1)) {
  new_customer = 0 ;
} else {
  new_customer = 1 ;
}

//Turn on debug mode eHUBu. 1 = eHUB return only info messages about your JSON request
let ehub_debug = 0 ;
if (data.debugMode == true) {
  ehub_debug = 1 ;
}


//Setup array for POST request to eHUB
var post_data = {
  'visitId': data.ehub,
  'orderId': data.transactionId,
  'currency': data.currency,
  'orderAmount': data.revenue,
  'couponCode': data.coupon,
  'couponDiscount': data.couponDiscount,
  'paymentMethod': data.paymentMethod,
  'newCustomer': new_customer,
  'debug': ehub_debug,
  'orderItems': []
};

//Convert products from ecommerce format to eHUB format a insert into POST request. If products exists.
if (typeof data.products != 'undefined') {
  data.products.forEach(function(produkt, index) {
    post_data.orderItems[index] = {
      'id': produkt.id,
      'name': produkt.name,
      'category': produkt.categories,
      'unitPrice': produkt.price,
      'quantity': produkt.quantity
    } ;
  });
}

//if debug_mode On, write complete JSON to Console
if (data.debugMode == true) {
  logToConsole(JSON.stringify({
    'Point': 'eHUB - POST data',
    'POST data': post_data
  }));
}

//Send data to eHUB
sendHttpRequest('https://ehub.cz/system/scripts/sale.php', (statusCode, headers, body) => {
  if (statusCode >= 200 && statusCode < 300) {
    if (data.debugMode == true) {
      logToConsole(JSON.stringify({
        'Point': 'eHUB - Success',
        'ResponseStatusCode': statusCode,
        'ResponseHeaders': headers,
        'ResponseBody': body
      }));
    }
    data.gtmOnSuccess();
  } else {
    if (data.debugMode == true) {
      logToConsole(JSON.stringify({
        'Point': 'eHUB - Failure',
        'ResponseStatusCode': statusCode,
        'ResponseHeaders': headers,
        'ResponseBody': body
      }));
    }
    data.gtmOnFailure();
  }},
  {
    headers: {'content-type': 'application/x-www-form-urlencoded'},
    method: 'POST', 
    timeout: 2000
   },
   JSON.stringify(post_data)
);


